<ui:composition
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:ui="http://java.sun.com/jsf/facelets">
	<style type="text/css">
	 .tab_user {
			width: 950px;
			margin: 0 auto;
		}
		
		
		.tab_user table{
			width: 850px;
			margin-left: auto;
			margin-right: auto;
		
		}
		
		.tab_user table tr{
			line-height: 30px;
			height: 30px;
			
		}
		
		.tab_user table tr td{
			border-left:1px solid;
			border-bottom:solid 1px;
			height: 30px;
			text-align: center;
		}
	</style>
	<div class="tab_user">
		  <table style="border-top: solid 1px;border-right: solid 1px;" cellpadding="0" cellspacing="0">
		     <tr style="background-color: #35d883;">
		       <td style="width: 215px;">用户名</td>
		       <td style="width: 215px;">电话</td>
		       <td style="width: 215px;">邮箱</td>
		     </tr>
		      <c:forEach var="user" items="#{sessionScope.list}" >
		         <tr style="background-color: #ccced4;">
			       <td style="width: 215px;">#{user.userName}</td>
			       <td style="width: 215px;">#{user.userPhone}</td>
			       <td style="width: 215px;">#{user.userEmail} </td>
			     </tr>
		      </c:forEach>
		  </table>
		</div>
</ui:composition>